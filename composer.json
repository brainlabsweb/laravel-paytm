{
    "name": "brainlabsweb/laravel-paytm",
    "description": "Paytm gateway for Laravel Applications",
    "license": "MIT",
    "keywords": ["laravel paytm","Mahesh","brainlabsweb","paytm"],
    "authors": [
        {
            "name": "Mahesh",
            "email": "brainlabs2010@gmail.com"
        }
    ],
    "require": {
        "ext-openssl": "*",
        "php": "^7.1.3",
        "illuminate/support": "5.5.* || 5.6.* || 5.7.* || 5.8.*"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Brainlabsweb\\Paytm\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brainlabsweb\\Paytm\\PaytmServiceProvider"
            ],
            "aliases": {
                "Paytm": "Brainlabsweb\\Paytm\\Paytm"
            }
        }
    }
}
